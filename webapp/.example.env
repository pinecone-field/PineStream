# Pinecone API key
# Get it from https://app.pinecone.io/
PINECONE_API_KEY=your-pinecone-api-key-here

# Groq  API key
# Get it from https://console.groq.com/
GROQ_API_KEY=your-groq-api-key-here

# Database Configuration
# Database file to use (movies.db, movies-small.db, etc.)
#DATABASE_FILE=movies.db
DATABASE_FILE=movies_small.db

# Embedding Generation Configuration
# Maximum chunks per batch for embedding generation (default: 50)
MAX_CHUNKS_PER_BATCH=50
# Maximum concurrent batches for embedding generation (default: 1)
MAX_CONCURRENT_BATCHES=1